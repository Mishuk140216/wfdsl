{
	"functions": [
		{
			"org": "srlab",
			"package": "demux",
			"module": "app.biowl.libraries.demux.adapter",
			"group": "Analysis",
			"name": "demux",
			"internal": "run_demux",
			"runmode": "local",
			"desc": "Demultiplex any number of FASTA or a FASTQ files based on a list of barcodes.",
			"returns": "folder",
			"example": "data = demux.demux(barcode, [data])",
			"example2": "data = demux.demux(barcode=, data=[data], [outdir=])",
			"params": [
				{
					"name": "barcode",
					"type": "file"
				},
				{
					"name": "data",
					"type": "file[]"
				}
			]
		}
	]
}
